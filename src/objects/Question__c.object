        <externalId>false</externalId>
        <label>Active</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Difficulty__c</fullName>
        <externalId>false</externalId>
        <label>Difficulty</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Basic</fullName>
                    <default>true</default>
                    <label>Basic</label>
                </value>
                <value>
                    <fullName>Intermediate</fullName>
                    <default>false</default>
                    <label>Intermediate</label>
                </value>
                <value>
                    <fullName>Advanced</fullName>
                    <default>false</default>
                    <label>Advanced</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Num_Asked__c</fullName>
        <defaultValue>0</defaultValue>
        <description>Counts the number of times the question has been used in an exam.</description>
        <externalId>false</externalId>
        <label>Num_Asked</label>
        <precision>5</precision>
        <required>true</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Num_Correct__c</fullName>
        <defaultValue>0</defaultValue>
        <description>Number of times the question has been correctly answered.</description>
        <externalId>false</externalId>
        <label>Num_Correct</label>
        <precision>5</precision>
        <required>true</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Percent_Correct__c</fullName>
        <externalId>false</externalId>
        <formula>( Num_Correct__c / Num_Asked__c ) * 100</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Percent_Correct</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
